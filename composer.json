{
    "name": "fredbradley/laravel-health-check-200",
    "description": "Laravel Health Check 200 is a Health Check which checks for a 200 HTTP Status Code",
    "keywords": [
        "fredbradley",
        "laravel",
        "laravel-health-check-200"
    ],
    "homepage": "https://github.com/fredbradley/laravel-health-check-200",
    "license": "MIT",
    "authors": [
        {
            "name": "Fred Bradley",
            "email": "code@fredbradley.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.14.0",
        "illuminate/contracts": "^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.8",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "FredBradley\\LaravelHealthCheck200\\": "src/",
            "FredBradley\\LaravelHealthCheck200\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FredBradley\\LaravelHealthCheck200\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "FredBradley\\LaravelHealthCheck200\\LaravelHealthCheck200ServiceProvider"
            ],
            "aliases": {
                "LaravelHealthCheck200": "FredBradley\\LaravelHealthCheck200\\Facades\\LaravelHealthCheck200"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}